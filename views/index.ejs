    <style type="text/css">
      body {
        background-color: #f0f0f0;
      }

      .search {
        width: 50%;
        position: relative;
        display: flex;
        margin: 0 auto;
        /*margin-left: 21%;*/
      }

      .searchTerm {
        width: 100%;
        border: 3px solid #00b4cc;
        border-right: none;
        padding: 5px;
        height: 36px;
        border-radius: 5px 0 0 5px;
        outline: none;
        color: #9dbfaf;
      }

      .searchTerm:focus {
        color: #00b4cc;
      }

      .searchButton {
        width: 40px;
        height: 36px;
        border: 1px solid #00b4cc;
        background: #00b4cc;
        text-align: center;
        color: #fff;
        border-radius: 0 5px 5px 0;
        cursor: pointer;
        font-size: 20px;
      }
      .allShop__card .row {
        height: 330px;
        overflow-x: auto;
        /* flex-wrap: nowrap; */
      }

      .single__shop {
        border: 1px solid gainsboro;
        height: 100px;
        width: 262px;
        display: inline-block;
      }
      .single__shop:hover {
        box-shadow: -1px -1px 10px 5px rgba(0, 0, 0, 0.2);
      }
      .single__shop a:hover {
        text-decoration: none;
      }

      .single__shop__details {
        border-left: 1px solid gainsboro;
        width: 150px;
        height: 100px;
        color: black;
      }

      .modal1 {
        position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%) scale(0);
        transition: 200ms ease-in-out;
        border: none;
        z-index: 10;
        background-color: white;
        width: 500px;
        max-width: 80%;
      }
      .modal1.active {
        transform: translate(-50%, -50%) scale(1);
      }
      .modal1-header {
        padding: 10px 15px;
        display: flex;
        flex-direction: row-reverse;
        justify-content: space-between;
        align-items: center;
      }
      .modal1-header .close-button {
        cursor: pointer;
        border: none;
        outline: none;
        background: none;
        font-size: 1.25rem;
        font-weight: bold;
      }
      .modal1-body {
        padding: 10px 15px;
      }
      #overlay {
        position: fixed;
        opacity: 0;
        transition: 200ms ease-in-out;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background-color: rgba(0, 0, 0, 0.5);
        pointer-events: none;
      }
      #overlay.active {
        opacity: 1;
        pointer-events: all;
      }
      @media only screen and (max-width: 600px) {
        #navbarSupportedContent {
          height: 100%;
          z-index: 0.3;
        }
        .search {
          padding-top: 15px;
          width: 100%;
          height: 100%;
          margin: 0 auto;
        }
      }
    </style>
