    <script type="text/javascript">
      const openModalButtons = document.querySelectorAll("[data-modal-target]");
      const closeModalButtons = document.querySelectorAll(
        "[data-close-button]"
      );

      const overlay = document.getElementById("overlay");

      openModalButtons.forEach((button) => {
        button.addEventListener("click", () => {
          const modal = document.querySelector(button.dataset.modalTarget);
          openModal(modal);
        });
      });

      closeModalButtons.forEach((button) => {
        button.addEventListener("click", () => {
          const modal = button.closest(".modal1");
          closeModal(modal);
        });
      });

      function openModal(modal) {
        if (modal == null) return;
        modal.classList.add("active");
        overlay.classList.add("active");
        var couponValue = document.getElementById("couponValue").value;
        console.log(couponValue);
        document.getElementById("couponData").innerText = couponValue;
      }

      function closeModal(modal) {
        if (modal == null) return;
        modal.classList.remove("active");
        overlay.classList.remove("active");
      }
    </script>

    <script type="text/javascript">
      const search = document.getElementById("search");
      const matchList = document.getElementById("match-list");

      const searchStates = async (searchText) => {
        const res = await fetch("https://couponsite2020.herokuapp.com/allStoresNew");
        const states = await res.json();
        console.log(searchText);
        let matches = states.filter((state) => {
          const regex = new RegExp(`${searchText}`, "gi");
          console.log(regex);
          return state.name && state.name.match(regex);
        });

        if (searchText.length === 0) {
          matches = [];
          matchList.style.display = "block";
        }

        outputHtml(matches);
      };
      //Show results in HTML

      const outputHtml = (matches) => {
        if (matches.length > 0) {
          document.getElementById("searchButtonId").disabled = false;
          const html = matches
            .map(
              (match) => `
              <a href="/singleShop/${match._id}">
            <div class="p-2 d-flex" style="border-bottom:1px solid black; height:45px">
              
                <img class="mr-2" style="width:40px; height:30px" src="${match.image}"/>
              <h4 style="font-size:15px; color:black">${match.name}</h4>
              
                
            </div>
          </a>
          `
            )
            .join("");
          matchList.innerHTML = html;
        } else if (matches.length == 0) {
          matchList.innerHTML = `<div class="p-2"><h4>nothing to show</h4></div>`;
          document.getElementById("searchButtonId").disabled = true;
        }
      };

      search.addEventListener("input", () => searchStates(search.value));
    </script>

    <script>
      const dropdownId = document.getElementById("dropdownCategory");
      const categoriesList = async () => {
        const res = await fetch("http://localhost:8081/categoriesIndex");
